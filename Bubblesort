package Practice;

public class Bubblesort {
	public static void main(String[] args) {
		
	
	int arr[]= {4,5,2,1,6,9,54};
	
	for(int i = 0 ; i <arr.length-1;i++) {
		for (int j = 0;j<arr.length-i-1;j++) {
			if(arr[j+1]<arr[j]) {
				int temp = arr[j+1];
				arr[j+1]=arr[j];
				arr[j]=temp ;
			}
		}
	}
	for (int k = 0 ; k <arr.length;k++) {
		System.out.println(arr[k]);
	}
}
	}
